{
  "expo": {
    "name": "GoVisaa",
    "slug": "GoVisaa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Govisaa.png",
    "scheme": "govisaa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "govisaa",
    "android": {
      "package": "com.govisaa.app",              
      "versionCode": 1,                            
      "adaptiveIcon": {
        "foregroundImage": "./assets/Govisaa.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/Govisaa.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/Govisaa.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2ec15e08-1ca8-4530-bf3e-0cb320d39440"
      }
    }
  }
}
